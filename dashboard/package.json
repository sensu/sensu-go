{
  "name": "dashboardd",
  "version": "0.1.0",
  "private": true,
  "files": [
    "build",
    "config",
    "scripts",
    "src"
  ],
  "engines": {
    "node": ">= 8",
    "npm": ">= 5.2.0",
    "yarn": "^1.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sensu/sensu-go/"
  },
  "scripts": {
    "audit-deps": "node scripts audit-deps",
    "check-version": "node scripts check-yarn-version",
    "prettier": "./node_modules/.bin/prettier --write '**/*.js'",
    "build:vendor": "./node_modules/.bin/webpack -r esm --config config/vendor.webpack.config.js",
    "build:lib": "./node_modules/.bin/webpack -r esm --config config/lib.webpack.config.js",
    "build:app": "./node_modules/.bin/webpack -r esm --config config/app.webpack.config.js",
    "build": "yarn build:vendor && yarn build:lib && yarn build:app && yarn build:pack",
    "build:pack": "yarn pack --filename dashboard.tgz",
    "analyze": "./node_modules/.bin/webpack-bundle-analyzer stats.json -s gzip",
    "lint": "./node_modules/.bin/eslint .",
    "spec": "node scripts test.js --env=jsdom",
    "test": "yarn check-version && yarn build && yarn lint && yarn spec --coverage",
    "update-schema": "node scripts update-schema",
    "flow": "./node_modules/.bin/flow",
    "dev-server": "node scripts serve",
    "start": "NODE_ENV=development yarn build:vendor && NODE_ENV=development yarn build:lib && NODE_ENV=development yarn dev-server"
  },
  "dependencies": {
    "@10xjs/date-input-controller": "0.1.6",
    "@10xjs/form": "^0.1.7",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-react-jsx-self": "^7.0.0",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
