{
  "name": "dashboardd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-relay": "^1.4.1",
    "babel-preset-react-app": "^3.1",
    "babel-runtime": "^6.23.0",
    "brcast": "^3.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "classnames": "^2.2.5",
    "codecov": "^2.3.0",
    "css-loader": "^0.28.4",
    "deepmerge": "^2.0.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-graphql": "^1.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "farce": "^0.2.1",
    "file-loader": "^1.1.4",
    "found": "^0.3.4",
    "found-relay": "^0.3.0-alpha.10",
    "graphiql": "^0.11.11",
    "graphql": "^0.12.0",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-reports": "^1.1.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "jest-fetch-mock": "^1.2.1",
    "jest-localstorage-mock": "^2.0.1",
    "jss": "^9.5",
    "lint-staged": "^6.1.0",
    "lodash": "^4.17.4",
    "material-ui": "v1.0.0-beta.30",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.19.1",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.7.3",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dev-utils": "^5.0",
    "react-dom": "^16.0.0",
    "react-jss": "~8.2",
    "react-redux": "^5.0.6",
    "react-relay": "~1.4.1",
    "react-tap-event-plugin": "^3.0.2",
    "recompose": "^0.26",
    "redux": "^3.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-freeze": "^0.1.5",
    "redux-thunk": "^2.2.0",
    "relay-compiler": "~1.4.1",
    "relay-runtime": "~1.4.1",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "typeface-roboto": "^0.0.54",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.1",
    "webpack-dev-server": "^2.8.2",
    "webpack-manifest-plugin": "^1.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "postinstall": "yarn run precompile",
    "prestart": "yarn precompile",
    "pretest": "yarn precompile",
    "prettier": "./node_module/.bin/prettier --write src/**/*.js",
    "lint": "./node_modules/.bin/eslint src --max-warnings 0",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "quality": "yarn lint && yarn test",
    "compile-queries": "./node_modules/.bin/relay-compiler --src ./src --schema ./data/schema.json",
    "update-schema": "go run scripts/updateSchema.go",
    "precompile": "npm run update-schema && npm run compile-queries"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "relay"
    ]
  },
  "proxy": {
    "/auth": {
      "target": "http://localhost:8080"
    },
    "/graphql": {
      "target": "http://localhost:8080"
    }
  }
}
