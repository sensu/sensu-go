  "scripts": {
    "prettier": "npx prettier --write '**/*.js'",
    "start": "node scripts serve",
    "build": "node scripts build",
    "analyze": "npx webpack-bundle-analyzer stats.json -s gzip",
    "lint": "npx eslint .",
    "spec": "node scripts test.js --env=jsdom",
    "test": "yarn build && yarn lint && yarn spec --coverage",
    "update-schema": "node scripts updateSchema",
    "flow": "npx flow"
  },
