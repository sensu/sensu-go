[version]
0.84.0

[lints]
all=error

[libs]
flow-typed

[ignore]
.*/node_modules/graphql/.*/.*\.js\.flow$
.*/node_modules/graphql-tag/.*\.flow\.js$

[options]
emoji=true
include_warnings=true
suppress_comment=\\(.\\|\n\\)*\\$ExpectError
module.ignore_non_literal_requires=true

module.name_mapper='^\($\|/.*$\)' -> '<PROJECT_ROOT>/src\1'
